{
  "name": "cart",
  "targetType": "executable",
  "configurations": [
    {
      "name": "raw",
      "dflags": [
        "-betterC"
      ],
      "lflags": [
        "--strip-all",
        "--allow-undefined",
        "--stack-first",
        "--import-memory",
        "--initial-memory=65536",
        "--max-memory=65536",
        "--no-entry",
        "-zstack-size=14752"
      ]
    },
    {
      "name": "wasi",
      "dflags": [
        "-betterC",
        "-Xcc",
        "$WASI_SDK_PATH/share/wasi-sysroot"
      ],
      "lflags": [
        "--strip-all",
        "--allow-undefined",
        "--stack-first",
        "--import-memory",
        "--initial-memory=65536",
        "--max-memory=65536",
        "--no-entry",
        "-zstack-size=14752",
        "$WASI_SDK_PATH/share/wasi-sysroot/lib/wasm32-wasi/libc.a"
      ]
    }
  ]
}
